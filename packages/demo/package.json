{
  "name": "@neat-js/demo",
  "version": "0.0.1",
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/heygrady/neat-js.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "yarn clean && concurrently --timings --prefix --names \"cjs,esm,tsc\" \"yarn build:cjs\" \"yarn build:esm\" \"yarn build:types\"",
    "build:cjs": "mkdir -p dist/cjs && echo '{\"type\":\"commonjs\",\"main\":\"./index.js\"}' >> dist/cjs/package.json && swc src -d dist/cjs --source-maps --copy-files -C module.type=commonjs",
    "build:esm": "swc src -d dist/esm --source-maps --copy-files -C module.type=es6",
    "build:types": "tsc --declaration --emitDeclarationOnly --project tsconfig.types.json --outDir dist/types",
    "clean": "rimraf dist coverage",
    "coverage": "vitest --coverage",
    "debug": "yarn build && nodemon --exec 'node --inspect=0.0.0.0:9229 ./dist/esm/index.js'",
    "dev": "yarn clean && concurrently --timings --prefix --names \"cjs,esm,tsc\" \"yarn dev:cjs\" \"yarn dev:esm\" \"yarn dev:types\"",
    "dev:cjs": "mkdir -p dist/cjs && echo '{\"type\":\"commonjs\",\"main\":\"./index.js\"}' >> dist/cjs/package.json && swc --watch src -d dist/cjs --source-maps --copy-files -C module.type=commonjs",
    "dev:esm": "swc --watch src -d dist/esm --source-maps --copy-files -C module.type=es6",
    "dev:types": "tsc --watch --declaration --emitDeclarationOnly --project tsconfig.types.json --outDir dist/types",
    "format": "eslint \"**/*.{md,cjs,mjs,js,jsx,cts,.ts,ts,tsx,json}\" --fix",
    "lint": "eslint \"**/*.{md,cjs,mjs,js,jsx,cts,.ts,ts,tsx,json}\"",
    "precommit": "lint-staged",
    "test": "vitest"
  },
  "dependencies": {
    "@neat-js/core": "^0.0.1",
    "@neat-js/dataset-environment": "^0.0.1",
    "@neat-js/environment": "^0.0.1",
    "@neat-js/evaluator": "^0.0.1",
    "@neat-js/evolution": "^0.0.1",
    "@neat-js/executor": "^0.0.1",
    "@neat-js/neat": "^0.0.1",
    "@neat-js/worker-evaluator": "^0.0.1",
    "@swc/helpers": "^0.5.1"
  },
  "devDependencies": {
    "@heygrady/eslint-config": "^1.5.0",
    "@heygrady/tsconfig-bases": "^1.0.5",
    "@neat-js/phenotype": "^0.0.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.80",
    "@types/eslint": "^8",
    "@types/prettier": "^2",
    "@types/seedrandom": "^3",
    "@vitest/coverage-v8": "^0.34.3",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3",
    "vitest": "^0.34.3"
  },
  "lint-staged": {
    "*.{md,cjs,mjs,js,jsx,cts,.ts,ts,tsx,json}": [
      "eslint --fix"
    ]
  },
  "browserslist": [
    "node >=16"
  ],
  "engines": {
    "node": ">=16"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "yarn@3.6.3"
}
