{
  "name": "@neat-evolution/worker-threads",
  "version": "0.4.2",
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/neat-evolution/neat-evolution.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": {
        "node": "./dist/esm/node.js",
        "browser": "./dist/esm/browser.js",
        "default": "./dist/esm/node.js"
      },
      "require": {
        "node": "./dist/cjs/node.js",
        "browser": "./dist/cjs/browser.js",
        "default": "./dist/cjs/node.js"
      }
    },
    "./browser": {
      "types": "./dist/types/browser.d.ts",
      "import": "./dist/esm/browser.js",
      "require": "./dist/cjs/browser.js"
    },
    "./node": {
      "types": "./dist/types/node.d.ts",
      "import": "./dist/esm/node.js",
      "require": "./dist/cjs/node.js"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "yarn clean && concurrently --timings --prefix --names \"cjs,esm,tsc\" \"yarn build:cjs\" \"yarn build:esm\" \"yarn build:types\"",
    "build:cjs": "mkdir -p dist/cjs && echo '{\"type\":\"commonjs\",\"main\":\"./index.js\"}' >> dist/cjs/package.json && swc src -d dist/cjs --source-maps --strip-leading-paths --copy-files -C module.type=commonjs",
    "build:esm": "swc src -d dist/esm --source-maps --strip-leading-paths --copy-files -C module.type=es6",
    "build:types": "tsc --declaration --emitDeclarationOnly --project tsconfig.types.json --outDir dist/types",
    "clean": "rimraf dist coverage",
    "coverage": "vitest --coverage",
    "debug": "yarn build && nodemon --exec 'node --inspect=0.0.0.0:9229 ./dist/esm/index.js'",
    "dev": "yarn clean && concurrently --timings --prefix --names \"cjs,esm,tsc\" \"yarn dev:cjs\" \"yarn dev:esm\" \"yarn dev:types\"",
    "dev:cjs": "mkdir -p dist/cjs && echo '{\"type\":\"commonjs\",\"main\":\"./index.js\"}' >> dist/cjs/package.json && swc --watch src -d dist/cjs --strip-leading-paths --source-maps --copy-files -C module.type=commonjs",
    "dev:esm": "swc --watch src -d dist/esm --strip-leading-paths --source-maps --copy-files -C module.type=es6",
    "dev:types": "tsc --watch --declaration --emitDeclarationOnly --project tsconfig.types.json --outDir dist/types",
    "format": "eslint \"**/*.{md,cjs,mjs,js,jsx,cts,.ts,ts,tsx,json}\" --fix",
    "lint": "eslint \"**/*.{md,cjs,mjs,js,jsx,cts,.ts,ts,tsx,json}\"",
    "precommit": "lint-staged",
    "test": "vitest --passWithNoTests --run"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.17"
  },
  "devDependencies": {
    "@heygrady/eslint-config": "^1.6.1",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.3",
    "@tsconfig/esm": "^1.0.5",
    "@tsconfig/node16": "^16.1.3",
    "@tsconfig/strictest": "^2.0.5",
    "@tsconfig/vite-react": "^2.0.1",
    "@vitest/coverage-v8": "^4.0.14",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "lint-staged": {
    "*.{md,cjs,mjs,js,jsx,cts,.ts,ts,tsx,json}": [
      "eslint --fix"
    ]
  },
  "browserslist": [
    "node >=16"
  ],
  "engines": {
    "node": ">=16"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "yarn@4.12.0"
}
